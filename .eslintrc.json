{
    "env": {
        "browser": true,
        "es2021": true,
        "webextensions": true
    },
    "extends": ["google", "eslint-config-prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "no-undef": "off",
        "no-unused-vars": "off",
        "require-jsdoc": "off"
    }
}
